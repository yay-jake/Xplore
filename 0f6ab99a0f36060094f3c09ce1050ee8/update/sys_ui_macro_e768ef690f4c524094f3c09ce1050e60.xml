                </div>

                <div id="node_log_pane" role="tabpanel" class="tab-pane fade">
                  <input type="hidden" id="node_log_url" />
                  <iframe id="node_log_frame" style="border: 0; width: 100%"></iframe>
                </div>
              </div><!-- /.tab-content -->

            </div>
          </div><!-- /#output -->
        </div><!-- /#workbench -->
